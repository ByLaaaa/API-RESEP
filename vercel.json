{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/Food",
            "dest": "/index.js"
        },
        {
            "src": "/img/(.*)",
            "dest": "/index.js"
        }
    ]
}